<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#5b8207" />
            <stroke
                android:width="1dp"
                android:color="#6d9e04" />
            <corners
                android:radius="3dp" />                
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#97db04"
				android:endColor="#7eb703"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#425f02" />
            <corners
                android:radius="4dp" />
        </shape>
    </item>
</selector>


